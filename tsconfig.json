{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "CommonJS",
        "noImplicitAny": false,
        "target": "ES2016",
        "sourceMap": true,
        "strict": false,
        "typeRoots": [
            "node_modules/@types",
            "typings"
        ],
        "lib": [
            "es6",
            "dom",
            "es2015.promise"
        ],
        "outDir": "bin",
    },
    "exclude": [
        "node_modules",
        "bin",
        "resource"
    ]
}
